script ChampionsGauntlet_BattleRoom_EventScript_ChampionsEnter
{
    showtrainersprite(FACILITY_CLASS_IONO, 20, 9)  
    msgbox(ChampionsGauntlet_BattleRoom_Text_PlayerIntro, MSGBOX_DEFAULT)
    closemessage  
    hidemonpic
    applymovement(LOCALID_ARCEUS_PLAYER, ChampionsGauntlet_DomePreBattleRoom_Movement_PlayerWalkToPodium)
    waitmovement(0)
    showtrainersprite(FACILITY_CLASS_IONO, 20, 9)  
    msgbox(ChampionsGauntlet_BattleRoom_Text_PreRivalIntro, MSGBOX_DEFAULT)
    closemessage  
    call(ChampionsGauntlet_BattleRoom_RivalIntroAndEntrance)
    end
}

script ChampionsGauntlet_BattleRoom_EventScript_FightEnd
{
    // Congrats for both and chuchu
    special(RestorePlayerStateAfterWarp)
    setvar(VAR_TEMP_2, 0)
    if(flag(FLAG_GRAND_FINALS)){
        call(ChampionsGauntlet_BattleRoom_EventScript_AddGrandFinalAudience)
        showtrainersprite(FACILITY_CLASS_IONO, 20, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_GrandFinalsOutroFirst, MSGBOX_DEFAULT)
	    closemessage    
        hidemonpic
        showtrainersprite(FACILITY_CLASS_OAK, -1, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_GrandsFinalsOutroSecond, MSGBOX_DEFAULT)
	    closemessage  
        hidemonpic 
        fadescreen(FADE_TO_BLACK)
        showtrainersprite(FACILITY_CLASS_ROTOM, 20, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_GrandsFinalThird, MSGBOX_DEFAULT)
	    closemessage  
        hidemonpic
        clearflag(FLAG_GRAND_FINALS)
        special(SetCB2WhiteOut)
    } 
    if(flag(FLAG_SEMI_FINALS)){
        call(ChampionsGauntlet_BattleRoom_EventScript_AddSemiFinalAudience)
        showtrainersprite(FACILITY_CLASS_IONO, 20, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_SemiFinalsOutroFirst, MSGBOX_DEFAULT)
	    closemessage    
        hidemonpic
        showtrainersprite(FACILITY_CLASS_OAK, -1, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_SemiFinalsOutroSecond, MSGBOX_DEFAULT)
	    closemessage  
        hidemonpic 

        clearflag(FLAG_SEMI_FINALS)
        setflag(FLAG_GRAND_FINALS) 
        delay(1)
        releaseall

        warp(MAP_CHAMPIONS_GAUNTLET_DOME_PRE_BATTLE_ROOM, 5, 4)
    } 
    if(flag(FLAG_QUARTER_FINALS)){
        call(ChampionsGauntlet_BattleRoom_EventScript_AddAudience)
        showtrainersprite(FACILITY_CLASS_IONO, 20, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_QuarterFinalsOutroFirst, MSGBOX_DEFAULT)
	    closemessage    
        hidemonpic
        showtrainersprite(FACILITY_CLASS_OAK, -1, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_QuarterFinalsOutroSecond, MSGBOX_DEFAULT)
	    closemessage  
        hidemonpic 
        clearflag(FLAG_QUARTER_FINALS)
        setflag(FLAG_SEMI_FINALS)  
        delay(1)
        releaseall

        warp(MAP_CHAMPIONS_GAUNTLET_DOME_PRE_BATTLE_ROOM, 5, 4)
    } 
    if(flag(FLAG_QUALIFIERS)){
        call(ChampionsGauntlet_BattleRoom_EventScript_AddAudience)
        showtrainersprite(FACILITY_CLASS_IONO, 20, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_QualifierOutroFirst, MSGBOX_DEFAULT)
	    closemessage    
        hidemonpic
        showtrainersprite(FACILITY_CLASS_OAK, -1, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_QualifierOutroSecond, MSGBOX_DEFAULT)
	    closemessage  
        hidemonpic
        clearflag(FLAG_QUALIFIERS)
        setflag(FLAG_QUARTER_FINALS) 
        delay(1)
        releaseall

        warp(MAP_CHAMPIONS_GAUNTLET_DOME_PRE_BATTLE_ROOM, 5, 4)
    }
    end
}

