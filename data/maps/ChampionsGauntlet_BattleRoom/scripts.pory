script ChampionsGauntlet_BattleRoom_EventScript_TournamentIntroOpen
{
    lockall
    applymovement(LOCALID_PLAYER, ChampionsGauntlet_BattleRoom_Movement_SetInvisible)
    waitmovement(0)
    setvar(VAR_TEMP_2, 1)
    if(flag(FLAG_QUALIFIERS)){
        call(ChampionsGauntlet_BattleRoom_EventScript_AddAudience)
        showtrainersprite(FACILITY_CLASS_IONO, 20, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_QualifierIntroFirst, MSGBOX_DEFAULT)
	    closemessage    
        hidemonpic
        showtrainersprite(FACILITY_CLASS_OAK, -1, 9)  
        msgbox(ChampionsGauntlet_BattleRoom_Text_QualifierIntroSecond, MSGBOX_DEFAULT)
	    closemessage   
        hidemonpic
    } 
    if(flag(FLAG_QUARTER_FINALS)){
        call(ChampionsGauntlet_BattleRoom_EventScript_AddAudience)
        msgbox(ChampionsGauntlet_BattleRoom_Text_QuarterFinalsIntro, MSGBOX_DEFAULT)
	    closemessage    
    } 
    if(flag(FLAG_SEMI_FINALS)){
        call(ChampionsGauntlet_BattleRoom_EventScript_AddSemiFinalAudience)
        msgbox(ChampionsGauntlet_BattleRoom_Text_SemiFinalsIntro, MSGBOX_DEFAULT)
	    closemessage    
    } 
    if(flag(FLAG_GRAND_FINALS)){
        call(ChampionsGauntlet_BattleRoom_EventScript_AddGrandFinalAudience)
        msgbox(ChampionsGauntlet_BattleRoom_Text_GrandFinalsIntro, MSGBOX_DEFAULT)
	    closemessage    
    }
    goto(ChampionsGauntlet_BattleRoom_EventScript_ChampionsEnter)
}


