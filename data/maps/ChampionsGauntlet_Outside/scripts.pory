script Gary_Selection
{
    message("Gary: An Indigo and Johto League Champion.")
    multichoice(25, 8, MULTI_CHAMPION_SELECTION, 0)
    closemessage
    if (var(VAR_RESULT) == 0){
        showtrainersprite(FACILITY_CLASS_ROTOM, 20, 7)
        msgbox(ChampionsGauntlet_Outside_Text_AccessingDatabase, MSGBOX_DEFAULT)
        closemessage
        hidemonpic  
        call(Gary_Pokemons)
        special(RetrievePokemonTeam)
        special(AccessPokemonChampionRoster)
     }
    elif (var(VAR_RESULT) == 1){
        call(Gary_Selected_Champion)
    }
}

script Gary_Selected_Champion
{
    msgbox("Do you want to follow Gary?", MSGBOX_YESNO)
    if(var(VAR_RESULT) == YES){
        // Asking Champion to follow them around
        special(SetPlayerGenderMale)
        setvar(VAR_TEMP_0, 0x1)
        special(SetPlayerCurrentPlayerName)
        call(Gary_Pokemons)
        call(ChampionsGauntlet_Outside_GiveCompleteDex)
        special(ChangeSpriteToGary)
        playfanfare(FANFARE_OBTAIN_ITEM)

        call(ChampionsGauntlet_Outside_ClearFlags)
        createfollowernpc(OBJ_EVENT_GFX_BRENDAN_NORMAL, FNPC_ALL, ChampionsGauntlet_Outside_FollowerRotom)
        removeobject(LOCALID_GARY)
        removeobject(LOCALID_BLASTOISE)
        msgbox("Now Following Gary!")
    } else {
        showtrainersprite(FACILITY_CLASS_ROTOM, 20, 7)
        msgbox(ChampionsGauntlet_Outside_Text_LetsNotWasteTime, MSGBOX_DEFAULT)
        closemessage
        hidemonpic  
    }
}
