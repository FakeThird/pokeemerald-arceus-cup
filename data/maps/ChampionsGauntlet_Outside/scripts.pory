script ChampionsGauntlet_Outside_GiveCompleteDex
{
    call(ChampionsGauntlet_Outside_SetFlags)

    setvar(VAR_TEMP_0, SPECIES_BULBASAUR)
    
CompleteDexLoop:
    setseenmon(VAR_TEMP_0)
    setcaughtmon(VAR_TEMP_0)

    addvar(VAR_TEMP_0, 0x0001)

    if (var(VAR_TEMP_0) != SPECIES_MIMIKYU_BUSTED_TOTEM) {
        goto(CompleteDexLoop) 
    }
    setseenmon(VAR_TEMP_0)
    setcaughtmon(VAR_TEMP_0)
    
    return
}

script ChampionsGauntlet_Outside_ClearFlags
{
    removeallitem(ITEM_MEGA_RING)
    removeallitem(ITEM_TERA_ORB)
    setflag(FLAG_CHOSEN_ROTOM)

    if(flag(FLAG_CHOSEN_GARY)){
        clearflag(FLAG_CHOSEN_GARY)
        addobject(LOCALID_GARY)
        addobject(LOCALID_BLASTOISE)
    }
    if(flag(FLAG_CHOSEN_LANCE)){
        clearflag(FLAG_CHOSEN_LANCE)
        addobject(LOCALID_LANCE)
        addobject(LOCALID_DRAGONITE)
    }
    if(flag(FLAG_CHOSEN_RED)){
        clearflag(FLAG_CHOSEN_RED)
        addobject(LOCALID_RED)
        addobject(LOCALID_SNORLAX)
    }
    if(flag(FLAG_CHOSEN_STEVEN)){
        clearflag(FLAG_CHOSEN_STEVEN)
        addobject(LOCALID_STEVEN)
        addobject(LOCALID_METAGROSS)
    }
    if(flag(FLAG_CHOSEN_WALLACE)){
        clearflag(FLAG_CHOSEN_WALLACE)
        addobject(LOCALID_WALLACE)
        addobject(LOCALID_MILOTIC)
    }
    if(flag(FLAG_CHOSEN_CYNTHIA)){
        clearflag(FLAG_CHOSEN_CYNTHIA)
        addobject(LOCALID_CYNTHIA)
        addobject(LOCALID_GARCHOMP)
    }
    if(flag(FLAG_CHOSEN_ALDER)){
        clearflag(FLAG_CHOSEN_ALDER)
        addobject(LOCALID_ALDER)
        addobject(LOCALID_VOLCARONA)
    }
    if(flag(FLAG_CHOSEN_IRIS)){
        clearflag(FLAG_CHOSEN_IRIS)
        addobject(LOCALID_IRIS)
        addobject(LOCALID_HAXORUS)
    }
    if(flag(FLAG_CHOSEN_DIANTHA)){
        clearflag(FLAG_CHOSEN_DIANTHA)
        addobject(LOCALID_DIANTHA)
        addobject(LOCALID_GARDEVOIR)
    }
    if(flag(FLAG_CHOSEN_ASH)){
        clearflag(FLAG_CHOSEN_ASH)
        addobject(LOCALID_ASH)
        addobject(LOCALID_PIKACHU)
    }
    if(flag(FLAG_CHOSEN_LEON)){
        clearflag(FLAG_CHOSEN_LEON)
        addobject(LOCALID_LEON)
        addobject(LOCALID_CHARIZARD)
    }
    if(flag(FLAG_CHOSEN_GEETA)){
        clearflag(FLAG_CHOSEN_GEETA)
        addobject(LOCALID_GEETA)
        addobject(LOCALID_GLIMMORA)
    }
    additem(ITEM_MEGA_RING)
    additem(ITEM_TERA_ORB)

    destroyfollowernpc
}
