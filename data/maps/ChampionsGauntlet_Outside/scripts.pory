script ChampionsGauntlet_Outside_EventScript_ArrivalIntro
{
    lockall
    showtrainersprite(FACILITY_CLASS_ROTOM, 20, 7)
    msgbox(ChampionsGauntlet_Outside_Text_Intro, MSGBOX_DEFAULT)
    closemessage
    hidemonpic
    setvar(VAR_ARCEUS_START, 1)
    releaseall
    end
}

text ChampionsGauntlet_Outside_Text_Intro
{
    "Finally, I'm here! Now let us look for\n"
	"a Champion or a Trainer to make a story\l"
	"out of, before I get reprimanded--\l"
    "ROTOM On Site!!!$"
}

script ChampionsGauntlet_Outside_CantEnter
{
    lockall
    if(!flag(FLAG_CHOSEN_ROTOM)){
        showtrainersprite(FACILITY_CLASS_ROTOM, 20, 7)
        msgbox(ChampionsGauntlet_Outside_Text_CantEnter, MSGBOX_DEFAULT)
        closemessage
        hidemonpic
        applymovement(LOCALID_PLAYER, ChampionsGauntlet_Outside_Movement_CantEnter)
        waitmovement(0)
    }
    releaseall 
    end
}
